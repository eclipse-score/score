{
  "name": "Esbonio",
  "programArgs": {
    "default": "sh -c \"export PYTHONPATH\u003d$PROJECT_DIR$/..; cd $PROJECT_DIR$/../docs; . $PROJECT_DIR$/../.venv_docs/bin/activate; python $PROJECT_DIR$/../.venv_docs/lib/python3.12/site-packages/esbonio/__main__.py\"\n"
  },
  "languageMappings": [
    {
      "language": "ReST",
      "languageId": "text.restructuredtext"
    }
  ],
  "fileTypeMappings": [
    {
      "fileType": {
        "name": "ReST"
      },
      "languageId": "text.restructuredtext"
    },
    {
      "fileType": {
        "patterns": [
          "*.rst"
        ]
      },
      "languageId": "text.restructuredtext"
    }
  ]
}